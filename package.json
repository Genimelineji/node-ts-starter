{
	"name": "node-ts-starter",
	"version": "1.0.0",
	"description": "Node with Typescript starter project.",
	"main": "./dist/index.js",
	"scripts": {
		"start:pm2": "pm2 start ./dist/index.js --name='node-ts-starter' -i 0",
		"start:dev": "./node_modules/.bin/nodemon --exec ./node_modules/.bin/ts-node index.ts",
		"start:compiled": "./node_modules/.bin/nodemon ./dist/index.js",
		"build": "tsc",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Genimelineji/node-ts-starter.git"
	},
	"keywords": [
		"nodejs",
		"typescript",
		"starter"
	],
	"author": "Genimelineji <genimelineji@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Genimelineji/node-ts-starter/issues"
	},
	"homepage": "https://github.com/Genimelineji/node-ts-starter#readme",
	"devDependencies": {
		"@types/node": "^4.0.35",
		"nodemon": "^1.14.7",
		"ts-node": "^4.1.0",
		"typescript": "^2.6.2"
	}
}